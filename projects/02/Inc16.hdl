/**
 * 16-bit incrementer:
 * out = in + 1 (arithmetic addition)
 */

CHIP Inc16 {
    IN in[16];
    OUT out[16];

    PARTS:
    // Put you code here:
    RetOne16(a=in, b=in, out=tmp);
    Add16(a=in, b=tmp, out=out);

    
}
